  delete module ifnlGenbankXmlPopulator
  delete module ifnlSideDataImporter
  
  create module -f modules/build/core/ifnlGenbankXmlPopulator.xml
  create module -f modules/build/core/ifnlSideDataImporter.xml

  # Import GenBank side data
  module ifnlGenbankXmlPopulator populate

  # Import metadata for IFNL curated sequences
  module ifnlSideDataImporter 
    populate -f tabular/refseq-lineage/ifnl-curated-side-data-mammalia-a.tsv
    populate -f tabular/refseq-lineage/ifnl-curated-side-data-mammalia-b.tsv
    populate -f tabular/refseq-lineage/ifnl-curated-side-data-aves.tsv
    populate -f tabular/refseq-lineage/ifnl-curated-side-data-testudines.tsv
    exit
