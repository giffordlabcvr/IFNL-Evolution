
project ifnl

  # Import lineage-level sources 
  run file glue/build/lineage/ifnlLoadSources.glue

  # Import sequence associated data for curated set
  run file glue/build/lineage/ifnlLoadCuratedSideData.glue

  # Create subclade references
  run file glue/build/lineage/ifnlSubcladeReferences.glue

  # Import lineage-level alignments 
  run file glue/build/lineage/ifnlAlignments.glue

  # Define lineage-level reference sequences
  #run file glue/build/lineage/ifnlLineageReferences.glue

  # Define constrained alignment tree
  run file glue/build/lineage/ifnlAlignmentTree.glue

  # Inherit feature locations
  #run file glue/build/lineage/inheritFeatureLocations.glue

  # Record feature coverage in alignments
  run file glue/build/lineage/ifnlLinegesRecordFeaturePresence.glue

  # Define variations 
  run file glue/build/lineage/ifnlVariations.glue

  # Scan variations 
  run script glue/build/lineage/ifnlScanVariationsGlycos1.js
  run script glue/build/lineage/ifnlScanVariationsGlycos2.js
  run script glue/build/lineage/ifnlScanVariationsGlycos3.js

  # Validate
  validate

  exit

