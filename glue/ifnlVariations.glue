    create variation n-linked-glycosylation-x -t aminoAcidRegexPolymorphism --labeledCodon 114 117 
    variation n-linked-glycosylation-x   
      set metatag REGEX_AA_PATTERN N[A-Z][ST]  #Asn–X–Ser/Thr consensus sequence, where X is any amino acid except proline (Pro).
    exit

  exit

  
exit


