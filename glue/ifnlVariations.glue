# REF_IFNLa_Mammal (IFNL4)
reference REF_IFNLa_Mammal_MASTER

  feature-location orf
  
    # Glycosylation site
    create variation n-linked-glycosylation -t aminoAcidRegexPolymorphism --labeledCodon 50 100
    variation n-linked-glycosylation    
      set metatag REGEX_AA_PATTERN N[A-Z][ST]  #Asn–X–Ser/Thr consensus sequence, where X is any amino acid except proline (Pro).
    exit
                                    
  exit
  
exit

reference REF_IFNLb2_Mammal_MASTER

  feature-location glycosylation-region-1

    # Glycosylation site O
    create variation n-linked-glycosylation-o -t aminoAcidRegexPolymorphism --labeledCodon 113 118 
    variation n-linked-glycosylation-o   
      set metatag REGEX_AA_PATTERN N[A-Z][ST]  #Asn–X–Ser/Thr consensus sequence, where X is any amino acid except proline (Pro).
    exit

    create variation n-linked-glycosylation-x -t aminoAcidRegexPolymorphism --labeledCodon 114 117 
    variation n-linked-glycosylation-x   
      set metatag REGEX_AA_PATTERN N[A-Z][ST]  #Asn–X–Ser/Thr consensus sequence, where X is any amino acid except proline (Pro).
    exit

  exit

