  delete module ifnlGenbankXmlPopulator
  delete module ifnlSideDataImporter
  delete module ifnlGlycosylationDataImporter

  
  create module -f modules/core/ifnlGenbankXmlPopulator.xml
  create module -f modules/core/ifnlSideDataImporter.xml
  create module -f modules/lineage/ifnlGlycosylationDataImporter.xml

  # Import GenBank side data
  module ifnlGenbankXmlPopulator populate

  # Import metadata for IFNL curated sequences
  module ifnlSideDataImporter 
    populate -f tabular/refseq-lineage/ifnl-curated-side-data.tsv
    exit

  # Import glycosylation data for IFNL reference sequences
  module ifnlGlycosylationDataImporter 
    populate -f tabular/refseq-lineage/ifnlb2-glycosylation.tsv
    exit
