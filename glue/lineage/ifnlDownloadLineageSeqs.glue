# Clean-up
delete module ncbiImporterRefSeqs
delete module ncbiImporterCurated

# Create modules
create module -f modules/ifnlNcbiImporterReferences.xml ncbiImporterRefSeqs
create module -f modules/ifnlNcbiImporterCurated.xml ncbiImporterCurated

# Download NCBI sequences 
module ncbiImporterRefSeqs import
module ncbiImporterCurated import

# Export sources
export source ncbi-refseqs
export source ncbi-curated
